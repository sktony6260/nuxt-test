{
  "name": "nuxt-test",
  "version": "1.0.0",
  "description": "My world-class Nuxt.js project",
  "author": "赵腾江",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --exec babel-node --watch server --watch imgServer",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "test": "ava -v --color"
  },
  "ava": {
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "child_process": "^1.0.2",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "express-async-handler": "^1.1.4",
    "fs": "0.0.1-security",
    "json-colorz": "^0.2.7",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "multer": "^1.4.1",
    "nuxt": "^2.0.0",
    "pstree.remy": "^1.1.0",
    "sharp": "^0.21.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "fs-temp": "^1.1.2",
    "jsdom": "^13.0.0",
    "nodemon": "^1.11.0"
  }
}
